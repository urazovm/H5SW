<%# Tabs available for both customer and jobs%>
<ul class="nav nav-tabs" id="custom_tab">
  <% @tabs.each do |tab| %>
    <li class=<%= (params[:tab].to_i == tab.id) ? 'active' : '' %> >
      <%= link_to (tab.name+" - #{tab.tab_type} "), new_custom_path(:tab => tab.id, :type => tab.tab_type) %>
    </li>
  <% end %>
    <li>
      <%= link_to "[+]New Tab", new_tab_customs_path(:type=> "Customer") %>
    </li>
</ul>
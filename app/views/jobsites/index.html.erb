<%= render '/layouts/customer_with_action' %>
<%= render 'tabbed' %>
<%= render 'search' %>
<table class="table">
  <table class="table table-striped">
    <tr>
      <th>Name</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Action</th>
    </tr>


    <% @jobsites.each do |jobsite| %>
      <tr>
        <td><%= jobsite.name %></td>
        <td><%= jobsite.city %></td>
        <td><%= jobsite.state %></td>
        <td><%= jobsite.zip %></td>
        <td><%= link_to 'Edit', edit_jobsite_path(jobsite), :class => "btn btn-mini btn-primary" %></td>
      </tr>
    <% end %>
    <tr>
      <td colspan="5">
        <%= will_paginate @jobsites %>
      </td>
    </tr>
  </table>
</table>

